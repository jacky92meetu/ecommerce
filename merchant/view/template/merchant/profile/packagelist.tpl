<!-- inline scripts related to this page -->
<script type="text/javascript">
    jQuery(function($) {

        $('div.pricing-choice .pricing-choice-plan-buy a.plan_buy_btn').click(function() {
            var $obj = $(this);
            var $html = $('<div>').append($obj.closest('.pricing-choice-plan-overview').clone());
            $html.find('.pricing-choice-plan-cta').remove();
            $html = $html.html();
            var $msg = $obj.closest('.pricing-choice-plan-overview').children(':lt(4)').map(function() {
                return $(this).text();
            }).get().join("\n");            
            bootbox.dialog({
                message: "Are you confirm to buy the following package?<br><br><center>" + $html + "</center>",
                title: "Buy Package",
                buttons: {
                  success: {
                    label: "Yes, Contact Me",
                    className: "btn-success",
                    callback: function() {                        
                        var $widget = $obj.closest('.widget-box');
                        if ($widget.length) {
                            $widget.widget_box('reload');
                        }
                        $.ajax({
                            type: 'POST',
                            url: '/merchant/profile/package',
                            data: {'msg':$msg,'isajax':true,'html':$html},
                            complete: function(xhr, status) {
                                setTimeout(function() {
                                    if ($widget.length) {
                                        $widget.trigger('reloaded.ace.widget');
                                    }
                                }, 2000);
                            },
                            success: function(data, status, xhr) {
                                try {
                                    data = JSON.parse(data);
                                } catch (e) {
                                    data = {};
                                    if (typeof bootbox === 'object') {
                                        bootbox.alert("Server Error!");
                                    } else {
                                        alert("Submission Error!");
                                    }
                                }
                                if (typeof data.text === 'string' && data.text.length > 0) {
                                    if (typeof bootbox === 'object') {
                                        bootbox.alert(data.text);
                                    } else {
                                        alert(data.text);
                                    }
                                }
                            }
                        });
                    }
                  },
                  danger: {
                    label: "Cancel",
                    className: "btn-default",
                    callback: function() {
                      
                    }
                  }
                }
              });
        });
        
        $('div.pricing-choice .pricing-choice-plan-buy a.plan_try_btn').click(function() {
            var $message = 'Thanks for choosing our “Trial Package”<br>Please proceed to upload your products & publish up to 30 days <br><br> Try it now and buy any plan later';
            bootbox.alert($message);
        });

    });
</script>